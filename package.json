{
  "name": "interactive-roadmap",
  "version": "1.0.0",
  "description": "Interactive Roadmap for Additive Manufacturing",
  "main": "src/main.js",
  "scripts": {
    "build:node": "webpack --config webpack.config.js",
    "build:browser": "webpack --config webpack.browser.config.js",
    "build": "npm run build:node && npm run build:browser",
    "start": "node build/interactiveRoadmap.js",
    "dev": "webpack --watch --config webpack.config.js & nodemon build/interactiveRoadmap.js"
  },
  "dependencies": {
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@babel/preset-env": "^7.23.9",
    "babel-loader": "^9.1.3",
    "nodemon": "^3.0.3",
    "webpack": "^5.90.1",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  }
}
